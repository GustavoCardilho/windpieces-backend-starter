{
  "name": "back-end-starter-project",
  "version": "0.0.0",
  "description": "a",
  "main": "index.js",
  "scripts": {
    "build:tsup-complete": "tsup src/server.ts --format cjs,esm --dts-resolve --minify --clean --sourcemap --legacy-output",
    "start": "tsup src/server.ts --format cjs,esm --dts-resolve --minify --clean --sourcemap --legacy-output && node dist/server.js",
    "build:tsc": "tsc",
    "dev": "tsx watch src/server.ts",
    "prepare": "husky install",
    "prettier:fix": "prettier --write .",
    "lint-staged": "lint-staged --allow-empty",
    "test": "vitest"
  },
  "author": "Kyoudan",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/Kyoudan/backend-starter-project.git"
  },
  "lint-staged": {
    "*.{ts}": [
      "prettier --write"
    ]
  },
  "bin": "./bin/cli.js",
  "devDependencies": {
    "@types/cors": "^2.8.14",
    "@types/express": "^4.17.17",
    "@types/morgan": "^1.9.5",
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "@typescript-eslint/parser": "^6.7.2",
    "dotenv": "^16.3.1",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "prettier": "^3.0.3",
    "tsup": "^7.2.0",
    "tsx": "^3.12.10",
    "typescript": "^5.2.2",
    "vitest": "^0.34.5"
  },
  "dependencies": {
    "colorette": "^2.0.20",
    "express": "^4.18.2",
    "morgan": "^1.10.0"
  }
}
